{
  "name": "@promptliano/ui",
  "version": "0.9.2",
  "description": "React component library for Promptliano with shadcn/ui components",
  "author": "Promptliano",
  "license": "MIT",
  "homepage": "https://github.com/promptliano/promptliano#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/promptliano/promptliano.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/promptliano/promptliano/issues"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "promptliano",
    "shadcn",
    "tailwind",
    "typescript"
  ],
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./styles": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./styles/globals": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "build": "bun run clean && NODE_ENV=production vite build && bun run build:css && bun run build:types",
    "build:css": "postcss src/styles/globals.css -o dist/index.css --verbose",
    "build:types": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run test:build && bun run build",
    "test:build": "bun run typecheck",
    "size": "bun run build && du -sh dist/*",
    "pack:dry": "npm pack --dry-run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.9.1",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@tanstack/react-table": "^8.20.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "1.0.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.511.0",
    "monaco-editor": "^0.52.2",
    "react-day-picker": "8.10.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.54.0",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^2.0.16",
    "react-syntax-highlighter": "^15.5.0",
    "recharts": "^2.12.7",
    "remark-gfm": "^4.0.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0",
    "zod": "^3.24.1"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@tanstack/react-form": "^1.19.2",
    "@tanstack/zod-form-adapter": "^0.42.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "6.3.3",
    "vite-plugin-lib-inject-css": "^2.2.2"
  }
}