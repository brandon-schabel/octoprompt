{
  "name": "@promptliano/services",
  "version": "0.9.2",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test src/ --timeout 5000",
    "test:unit": "bun test src/**/*.test.ts --timeout 5000",
    "test:integration": "bun test src/**/*.integration.test.ts --timeout 10000",
    "test:queue": "bun test src/queue-*.test.ts --timeout 10000",
    "test:coverage": "bun test src/ --timeout 5000 --coverage",
    "test:watch": "bun test src/ --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@promptliano/config": "workspace:*",
    "@promptliano/shared": "workspace:*",
    "@promptliano/storage": "workspace:*",
    "@promptliano/schemas": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}