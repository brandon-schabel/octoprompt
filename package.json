{
  "name": "promptliano-core",
  "module": "index.ts",
  "version": "0.9.2",
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/start-dev.ts",
    "dev:client": "bun run scripts/start-client-dev.ts",
    "dev:server": "bun run scripts/start-server-dev.ts",
    "stop": "bun run scripts/stop.ts",
    "build-binaries": "bun run scripts/build-binaries.ts",
    "format": "prettier --write .",
    "test:shared": "cd packages/shared && bun run test",
    "test:schemas": "cd packages/schemas && bun run test",
    "test:services": "cd packages/services && bun run test",
    "test:storage": "cd packages/storage && bun run test",
    "test:api-client": "cd packages/api-client && bun run test",
    "test:api-client:isolated": "cd packages/api-client && bun run test:isolated",
    "test:api-client:ai": "cd packages/api-client && bun run test:ai",
    "test:api-client:tickets": "cd packages/api-client && bun run test:tickets",
    "test:api-client:queues": "cd packages/api-client && bun run test:queues",
    "test:api-client:flow": "cd packages/api-client && bun run test:flow",
    "test:api-client:flow-system": "cd packages/api-client && bun run test:flow-system",
    "test:api-client:flow-complete": "cd packages/api-client && bun run test:flow-complete",
    "test:config": "cd packages/config && bun run test",
    "test:server": "cd packages/server && bun run test",
    "test:all": "bun run test:server && bun run test:shared && bun run test:schemas && bun run test:services && bun run test:storage && bun run test:api-client && bun run test:config",
    "test:queue": "cd packages/services && bun test src/queue-*.test.ts --timeout 10000",
    "test:queue:e2e": "bun run test-queue-system.ts",
    "test:local-models": "LMSTUDIO_BASE_URL=http://192.168.1.38:1234 bun test --timeout 30000 packages/services/src/tests/*.test.ts",
    "test:summarization": "bun test packages/services/src/tests/file-summarization.test.ts",
    "test:ai-integration": "LMSTUDIO_BASE_URL=http://192.168.1.38:1234 bun test:local-models",
    "docs:generate": "./scripts/generate-docs.sh",
    "docs:clean": "./scripts/generate-docs.sh --clean",
    "docs:serve": "./scripts/generate-docs.sh --serve",
    "docs:markdown": "./scripts/generate-docs.sh --markdown",
    "test:ai-e2e": "bun test packages/services/src/tests/e2e/summarization-workflow.test.ts --timeout 90000",
    "typecheck": "bun run typecheck:server && bun run typecheck:shared && bun run typecheck:schemas && bun run typecheck:services && bun run typecheck:storage && bun run typecheck:api-client && bun run typecheck:config && bun run typecheck:client && bun run typecheck:website",
    "typecheck:server": "cd packages/server && bun run typecheck",
    "typecheck:shared": "cd packages/shared && bun run typecheck",
    "typecheck:schemas": "cd packages/schemas && bun run typecheck",
    "typecheck:services": "cd packages/services && bun run typecheck",
    "typecheck:storage": "cd packages/storage && bun run typecheck",
    "typecheck:api-client": "cd packages/api-client && bun run typecheck",
    "typecheck:config": "cd packages/config && bun run typecheck",
    "typecheck:client": "cd packages/client && tsc --noEmit",
    "typecheck:website": "cd packages/website && bun run typecheck",
    "migrate:sqlite": "bun run scripts/migrate-to-sqlite.ts",
    "migrate:sqlite:dry": "bun run scripts/migrate-to-sqlite.ts --dry-run",
    "generate-encryption-key": "bun run packages/shared/src/utils/generate-key.ts",
    "migrate:encrypt-keys": "bun run packages/storage/src/migrations/encrypt-provider-keys.ts",
    "dev:website": "bun run scripts/start-website-dev.ts",
    "build:website": "cd packages/website && bun run build",
    "build:ui": "cd packages/ui && bun run build",
    "build:ui:bun": "cd packages/ui && bun run build:bun",
    "build:all": "bun run scripts/build-all.ts",
    "build:all:parallel": "bun run scripts/build-all.ts --parallel",
    "build:all:bun": "bun run scripts/build-all.ts --bun",
    "build:all:parallel:bun": "bun run scripts/build-all.ts --parallel --bun",
    "preview:website": "cd packages/website && bun run preview",
    "sync-version": "bun run scripts/sync-version.ts",
    "update-version": "bun run scripts/update-version.ts",
    "validate": "bun run scripts/validate-all.ts",
    "validate:quick": "bun run typecheck && bun run test:all",
    "benchmark": "./benchmarks/run-benchmarks.sh",
    "benchmark:gc": "./benchmarks/run-benchmarks.sh --gc",
    "benchmark:profile": "./benchmarks/run-benchmarks.sh --gc --profile",
    "benchmark:memory": "./benchmarks/run-benchmarks.sh --gc --memory-limit 4096",
    "benchmark:patterns": "bun --expose-gc run benchmarks/pattern-performance.ts",
    "benchmark:regression": "bun run benchmarks/regression-check.ts",
    "benchmark:analyze": "bun run benchmarks/regression-check.ts --threshold 5",
    "benchmark:bundle": "bun run benchmarks/bundle-analysis.ts",
    "benchmark:validate": "bun run benchmarks/validate-setup.ts"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@swc/core": "^1.13.1",
    "@swc/wasm": "^1.13.1",
    "@types/archiver": "^6.0.3",
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9",
    "@types/marked": "^6.0.0",
    "@types/path-browserify": "^1.0.3",
    "@types/picomatch": "^3.0.2",
    "archiver": "^7.0.1",
    "bun-types": "latest",
    "esbuild": "^0.25.8",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/openai-compatible": "^0.2.14",
    "@anthropic-ai/claude-code": "^1.0.22",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hey-api/client-fetch": "^0.10.2",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^0.19.8",
    "@hono/zod-validator": "^0.4.3",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@openrouter/ai-sdk-provider": "^0.4.6",
    "@promptliano/api-client": "workspace:*",
    "@promptliano/schemas": "workspace:*",
    "@promptliano/services": "workspace:*",
    "@promptliano/shared": "workspace:*",
    "@promptliano/storage": "workspace:*",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-adapter": "^1.129.8",
    "ai": "^4.3.16",
    "fast-xml-parser": "^5.2.4",
    "gray-matter": "^4.0.3",
    "hono": "^4.7.11",
    "hono-rate-limiter": "^0.4.2",
    "ignore": "^7.0.5",
    "isomorphic-dompurify": "^2.26.0",
    "js-yaml": "^4.1.0",
    "marked": "^16.1.1",
    "openai": "^4.104.0",
    "picomatch": "^4.0.2",
    "simple-git": "^3.28.0",
    "zod": "^3.25.56",
    "zod-validation-error": "^3.4.1"
  },
  "private": true
}